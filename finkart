function longestcommonsubstring(str1,str2)
{
    let longest="";
    let current ="";
    fot(let i=0;i<str.length;i++){
        for(let i=0;j<str2.length;j++)
{
    if(str1[i]==str2[j]){
             current+=str1[i];
             if(current.length>longest.length){
                 longest=current;
                 
             }
         } else{
             current="";
         }
    }
}
return longest;
console.log(longestcommonsubstring("abcdad","abcs"));